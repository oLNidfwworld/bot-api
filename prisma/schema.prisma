generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  id    Int     @default(autoincrement()) @id
  tg_uid Int 
  tg_name String  
  tg_avatar String @default("")
  tg_description String @default("")
} 

model Games {
  id Int @default(autoincrement()) @id
  g_name String 
}